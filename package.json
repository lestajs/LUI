{
  "name": "lui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "targets": {
    "esm": {
      "context": "browser",
      "outputFormat": "esmodule",
      "sourceMap": false,
      "optimize": true,
      "distDir": "./bundlers",
      "isLibrary": true
    }
  },
  "scripts": {

    "buttons": "parcel ./examples/components/buttons/index.html",
    "chip": "parcel ./examples/components/chip/index.html",
    "checkbox": "parcel ./examples/components/checkbox/index.html",
    "input": "parcel ./examples/components/input/index.html",
    "inputNumber": "parcel ./examples/components/inputNumber/index.html",
    "tags": "parcel ./examples/components/tags/index.html",
